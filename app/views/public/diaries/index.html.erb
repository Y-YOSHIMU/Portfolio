<h1>日記一覧</h1>

<table class="table">
  <thead>
    <tr>
      <th>タイトル</th>
    </tr>
  </thead>
  <tbody>
    <% @alldiary.each do |diary| %>
    <tr>
      <td>
        <%= link_to diary_path(diary.id) do %>
          <%= diary.title %>
        <% end %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @alldiary %>