<h1>お気に入り一覧</h1>

<table class="table">
	<thead>
		<tr>
			<th>タイトル</th>
		</tr>
	</thead>
	<tbody>
		<% @favorites.each do |favorite| %>
		<tr>
			<td>
				<%= link_to diary_path(favorite.diary_id) do %>
					<%= favorite.diary.title %>
				<% end %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>
<%= paginate @favorites %>